@model IEnumerable<job_management_final.employeer>

@{
    ViewBag.Title = "Index";
}

<h2>All companies</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.office_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contact_number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.office_address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.user.email)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.office_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.contact_number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.office_address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.user.email)
            </td>
            <td>
                @*@Html.ActionLink("Edit", "Edit", new { id=item.id_employeer }) |*@
                @Html.ActionLink("Details", "Details", new { id = item.id_employeer })
                @*@Html.ActionLink("Delete", "Delete", new { id=item.id_employeer })*@
            </td>
        </tr>
    }

</table>
